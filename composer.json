{
    "name" : "puzzle/amqp",
    "description" : "AMQP implementation based on PECL extension",
    "type" : "library",
    "license": "MIT",
    "authors" : [{
            "name" : "Nicolas Le Nardou",
            "email" : "nico.ln@gmail.com"
        }
    ],
    "keywords" : [
        "amqp",
        "asynchronous"
    ],
    "autoload" : {
        "psr-4" : {
            "Puzzle\\AMQP\\" : "src",
            "Puzzle\\AMQP\\Contexts\\": "features/bootstrap"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Puzzle\\AMQP\\" : "tests"
        }
    },
    "require" : {
        "php": "8.*",
        "puzzle/configuration" : "4.0.*",
        "puzzle/pieces" : "dev-clb-sf5",
        "symfony/console": "^5.4|^6.0",
        "symfony/event-dispatcher": "~5.4|^6.0",
        "swarrot/swarrot": "^4.0",
        "knplabs/gaufrette": "~0.2",
        "ramsey/uuid": "^4.0"
    },
    "require-dev" : {
        "phpunit/phpunit" : "~9.0",
        "puzzle/assert": "~1.1",
        "pimple/pimple": "~3.0",
        "odolbeau/rabbit-mq-admin-toolkit": "~5.2.2",
        "behat/behat": "~3.3",
        "symfony/debug": "~3.2",
        "doctrine/collections": "<=1.4.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/naoned/rabbit-mq-admin-toolkit"
        }
    ],
    "suggest": {
        "ext-amqp": "PECL AMQP extension is required",
        "ext-zip" : "PECL ZLib extension is required for GZip processor"
    }
}

{
    "name" : "puzzle/amqp",
    "description" : "AMQP implementation based on PECL extension",
    "type" : "library",
    "authors" : [{
            "name" : "Nicolas Le Nardou",
            "email" : "nico.ln@gmail.com"
        }
    ],
    "keywords" : [
        "aqmp",
        "asynchronous"
    ],
    "autoload" : {
        "psr-4" : {
            "Puzzle\\AMQP\\" : "src"
        }
    },
    "require" : {
        "php": ">=5.6",
        "puzzle/configuration" : "~1.7",
        "puzzle/pieces" : "~1.0",
        "symfony/console": ">=2.4",
        "swarrot/swarrot": "~2.1",
        "knplabs/gaufrette": "~0.2",
        "symfony/event-dispatcher": ">=2.4"
    },
    "require-dev" : {
        "phpunit/phpunit" : "~5.3",
        "empi89/php-amqp-stubs": "dev-master",
        "puzzle/assert": "~1.0"
    },
    "suggests": {
        "ext-amqp": "PECL AMQP extension is required"
    }
}
